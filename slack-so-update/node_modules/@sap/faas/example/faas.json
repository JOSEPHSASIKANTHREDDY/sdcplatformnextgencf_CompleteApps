{
  "project": "my-new-project",
  "version": "0.0.1",

  "runtime": "nodejs10",
  "library": "./lib",

  "secrets": {
    "my-new-sec": {
      "source": "./data/sec1"
    }
  },

  "functions": {
    "my-new-fnc": {
      "module": "index.js",
      "secrets": ["my-new-sec"]
    }
  },

  "triggers": {
    "my-new-srv": {
      "type": "HTTP",
      "function": "my-new-fnc"
    }
  }
}